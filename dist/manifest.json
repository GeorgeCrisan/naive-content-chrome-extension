{
    "name": "Naive content Chorme extension",
    "description": "Base Level Extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "contextMenus",
        "tabs",
        "scripting",
        "clipboardRead",
        "clipboardWrite"
    ],
    "action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon.png",
        "48": "iconBig.png",
        "128": "iconBig.png"
    },
    "background": {
        "service_worker": "ncce-background.js"
    }
}